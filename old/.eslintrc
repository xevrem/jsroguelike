{
	"env": {
		"node": true
	},
	"rules": {
		"block-scoped-var": 0,
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": false
			}
		],
		"camelcase": 1,
		"comma-spacing": [
			2,
			{
				"after": true,
				"before": false
			}
		],
		"comma-style": [
			2,
			"last"
		],
		"complexity": 0,
		"consistent-return": 0,
		"consistent-this": [
			1,
			"me"
		],
		"curly": [
			2,
			"all"
		],
		"default-case": 0,
		"dot-notation": 2,
		"eol-last": 2,
		"eqeqeq": 2,
		"func-names": 0,
		"func-style": [
			2,
			"expression"
		],
		"generator-star": [
			2,
			"start"
		],
		"global-strict": 0,
		"guard-for-in": 2,
		"handle-callback-err": [
			2,
			"error"
		],
		"key-spacing": [
			2,
			{
				"afterColon": true,
				"beforeColon": false
			}
		],
		"max-depth": 0,
		"max-len": 0,
		"max-nested-callbacks": 0,
		"max-params": 0,
		"max-statements": 0,
		"new-cap": 1,
		"new-parens": 2,
		"no-alert": 1,
		"no-array-constructor": 2,
		"no-bitwise": 1,
		"no-caller": 2,
		"no-catch-shadow": 0,
		"no-comma-dangle": 2,
		"no-cond-assign": 2,
		"no-console": 1,
		"no-constant-condition": 1,
		"no-control-regex": 0,
		"no-debugger": 2,
		"no-delete-var": 2,
		"no-div-regex": 0,
		"no-dupe-keys": 2,
		"no-else-return": 1,
		"no-empty": 1,
		"no-empty-class": 2,
		"no-empty-label": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-ex-assign": 0,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 1,
		"no-extra-semi": 2,
		"no-extra-strict": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 0,
		"no-func-assign": 2,
		"no-implied-eval": 2,
		"no-inline-comments": 0,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-label-var": 1,
		"no-labels": 0,
		"no-lone-blocks": 1,
		"no-lonely-if": 1,
		"no-loop-func": 2,
		"no-mixed-requires": 0,
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-multiple-empty-lines": [
			2,
			{
				max: 1
			}
		],
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-nested-ternary": 0,
		"no-new": 1,
		"no-new-func": 2,
		"no-new-object": 2,
		"no-new-require": 2,
		"no-new-wrappers": 2,
		"no-obj-calls": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-path-concat": 1,
		"no-plusplus": 2,
		"no-process-env": 0,
		"no-process-exit": 1,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-regex-spaces": 2,
		"no-reserved-keys": 0,
		"no-restricted-modules": 0,
		"no-return-assign": 2,
		"no-script-url": 1,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-shadow": 0,
		"no-shadow-restricted-names": 2,
		"no-space-before-semi": 2,
		"no-spaced-func": 2,
		"no-sparse-arrays": 1,
		"no-sync": 1,
		"no-ternary": 0,
		"no-trailing-spaces": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-undefined": 2,
		"no-underscore-dangle": 0,
		"no-unreachable": 1,
		"no-unused-expressions": 1,
		"no-unused-vars": [
			1,
			{
				"args": "after-used",
				"vars": "all"
			}
		],
		"no-use-before-define": 2,
		"no-var": 0,
		"no-void": 0,
		"no-warning-comments": [
			1,
			{
				"location": "start",
				"terms": [
					"todo"
				]
			}
		],
		"no-with": 2,
		"no-wrap-func": 2,
		"one-var": 2,
		"operator-assignment": [
			2,
			"always"
		],
		"padded-blocks": [
			2,
			"never"
		],
		"quote-props": [
			2,
			"as-needed"
		],
		"quotes": [
			1,
			"single"
		],
		"radix": 2,
		"semi": [
			2,
			"always"
		],
		"sort-vars": 0,
		"space-after-function-name": [
			2,
			"always"
		],
		"space-after-keywords": [
			2,
			"always"
		],
		"space-before-blocks": [
			2,
			"always"
		],
		"space-in-brackets": 0,
		"space-in-parens": 0,
		"space-infix-ops": 2,
		"space-return-throw-case": 2,
		"space-unary-ops": [
			2,
			{
				"nonwords": false,
				"words": true
			}
		],
		"spaced-line-comment": [
			1,
			"always"
		],
		"strict": 2,
		"use-isnan": 2,
		"valid-jsdoc": 0,
		"valid-typeof": 2,
		"vars-on-top": 0,
		"wrap-iife": [
			2,
			"outside"
		],
		"wrap-regex": 0,
		"yoda": "never"
	}
}